{
    "name": "tauseedzaman/laravel-ip-blocker",
    "description": "a simple package for blocking ips from your laravel application",
    "license": "MIT",
    "keywords": [
        "tauseedzaman",
        "laravel",
        "laravel-ip-location-block"
    ],
    "homepage": "https://github.com/tauseedzaman/tauseedzaman/laravel-ip-blocker",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tauseedzaman/tauseedzaman/laravel-ip-blocker/issues"
    },
    "authors": [
        {
            "name": "tauseedzaman",
            "email": "tauseedzaman@pm.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "tauseedzaman\\LaravelIpBlocker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tauseedzaman\\LaravelIpBlocker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "tauseedzaman\\LaravelIpBlocker\\IpBlockerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
